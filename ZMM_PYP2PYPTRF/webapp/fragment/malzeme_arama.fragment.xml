<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog noDataText="Malzememe bulunamadı.." title="Malzeme Seçiniz..." search="handleSearchMatnr" confirm="handleCloseMatnr"
		cancel="handleCloseMatnr" items="{ path : '/ZmmP2ptfShMatnrSet', sorter : { path : 'Matnr', descending : false } }">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="{Matnr}"/>
				<Text text="{Maktx}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Malzeme No"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="Malzeme Tanımı"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>