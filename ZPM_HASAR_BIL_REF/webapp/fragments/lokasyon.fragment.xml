		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="{Lokasyon}"/>
				<Text text="{Lokasyontxt}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Lokasyon"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="Lokasyon"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>