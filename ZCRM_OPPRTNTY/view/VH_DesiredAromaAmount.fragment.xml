<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:layout="sap.ui.layout">
    	<SelectDialog
		noDataText="{i18n>NODATATEXT}"
		title="{i18n>DESIREDAMOUNTTITLE}"
		search="onDesiredAromaAmountSearch"
		confirm="onDesiredAromaAmountSelect"
		cancel="onDesiredAromaAmountCancel"
		items="{path : '/CustomizingSearchHelpSet',
		        filters: [{path: 'FieldName', operator: 'EQ', value1: 'UNIT'}]
		       }">
    	<StandardListItem
			title="{Value}"
			description="{Key}"
			type="Active" />
    	</SelectDialog>
</core:FragmentDefinition>